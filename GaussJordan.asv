function xs = GaussJordan(mat)
    n = size(mat);
    n = n(1)
    xs = [];
    for i = 1 : n                 % pivot row
        for k = 1 : n            % rows (except for the pivot row)
            if k == i
              
                continue;
            end
            m = mat(k, i) / mat(i, i);
            for j = 1 : n+1     % columns
                mat(k, j) = mat(k, j) - m * mat(i, j);
            end
        end
        disp(mat);
    end
    
end